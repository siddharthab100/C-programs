#include<stdio.h>
int count=0;
void printMat(int [5][5],int);
int makeMat(int ,int , int , int [5][5]);
int main()
{int N=2,A[5][5];
int i,j;
for(i=1;i<=N;i++)
{for(j=1;j<=N;j++)
{
A[i][j]=0;
}
}
printMat(A,N);
makeMat(1,N,N,A);
return 0;}
void printMat(int A[5][5],int N)
{
int i,j;
for(i=1;i<=N;i++)
{for(j=1;j<=N;j++)
{
printf("%d\t",A[i][j]);
}
printf("\n");}
}
int makeMat(int row,int n, int N, int A[5][5])
{int j;
if(n==0)
{printf("\t\t%d\n",++count);
printMat(A,N);
return 1;
}

for(j=1;j<=N;j++)
{
if(A[row][j]==0)
{A[row][j]=1;
if(makeMat(row+1,n-1,N,A))
{A[row][j]=0;
}
}
}
}
